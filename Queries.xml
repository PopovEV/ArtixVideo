<QueryList>
  <Query num = "1" name = "Показать позиции за период времени" child = "" ischild = "false">
        <SQL>
            select *
            from pos_log_master
	    where date_begin &gt;= :date_beg and date_end  &lt;= :date_end
        </SQL>
	<parameters>
		<parameter value = "date_beg" name = "Начальная дата и время: " type = "datetime"/>
		<parameter value = "date_end" name = "Конечная дата и время: " type = "datetime"/>
	</parameters>
  </Query>
  <Query num = "2" name="Показать позицию по номеру чека" child = "" ischild = "false">
